local DiscordLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/pndxvo/scr/main/ui.lua"))()
local win = DiscordLib:Window("üê∑ FlukeDupe + Build a Zoo")
local serv = win:Server("MAIN", "")
local main = serv:Channel("FlukeDupe")

-- üì¶ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RS = game:GetService("ReplicatedStorage")
local TPService = game:GetService("TeleportService")

-- üåü ‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤
local selectedTarget = nil

----------------------------------------------------------
-- üîÑ Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
----------------------------------------------------------
local targetDropdown = main:Dropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏™‡πà‡∏á‡∏Ç‡∏≠‡∏á", {"..."}, function(playerName)
    selectedTarget = playerName
end)

local function updatePlayers()
    targetDropdown:Clear()
    targetDropdown:Add("...")
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            targetDropdown:Add(player.Name)
        end
    end
end

updatePlayers()
Players.PlayerAdded:Connect(updatePlayers)
Players.PlayerRemoving:Connect(updatePlayers)

main:Button("Refresh Players", function()
    updatePlayers()
    selectedTarget = nil
end)

----------------------------------------------------------
-- üçà ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Dupe Durian
----------------------------------------------------------
local function DupeDurian()
    if not selectedTarget then
        DiscordLib:Notification("‚ùå Error", "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô", "‡∏ï‡∏Å‡∏•‡∏á")
        return
    end

    local NAMEFRUITS = "Durian"
    local NANMEPLR = selectedTarget

    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ QuickSell
    RS:WaitForChild("Remote"):WaitForChild("FishingRE"):FireServer("SetEggQuickSell", {
        ["1"] = "\255",
        Diamond = false,
        ["3"] = true,
        ["2"] = false,
        ["5"] = false,
        ["4"] = false,
        ["6"] = false,
        Golden = false,
        Electirc = false,
        Fire = false,
        Dino = false,
        Snow = false
    })

    -- ‡∏£‡∏≠ GUI ‡πÇ‡∏´‡∏•‡∏î
    repeat task.wait(0.1) until LocalPlayer.PlayerGui and LocalPlayer.PlayerGui:FindFirstChild("Data") and LocalPlayer.PlayerGui.Data:FindFirstChild("Asset")

    -- ‡∏ß‡∏ô‡∏™‡πà‡∏á‡∏à‡∏ô‡∏´‡∏°‡∏î
    repeat
        task.wait()
        local targetPlayer = Players:FindFirstChild(NANMEPLR)
        if targetPlayer then
            pcall(function()
                RS:WaitForChild("Remote"):WaitForChild("GiftRE"):FireServer(targetPlayer)
            end)
        end
        if not LocalPlayer.PlayerGui or not LocalPlayer.PlayerGui.Data or not LocalPlayer.PlayerGui.Data.Asset then
            break
        end
    until not LocalPlayer.PlayerGui.Data.Asset:GetAttribute(NAMEFRUITS)
          or LocalPlayer.PlayerGui.Data.Asset:GetAttribute(NAMEFRUITS) <= 0

    DiscordLib:Notification("üçà Dupe Durian", "Dupe Durian ‡πÄ‡∏™‡∏£‡πá‡∏à‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß! ‡∏à‡∏∞‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å", "‡∏ï‡∏Å‡∏•‡∏á")

    -- ‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å
    pcall(function()
        TPService:TeleportToPlaceInstance(game.PlaceId, game.JobId)
    end)
end

main:Button("üçà Dupe Durian‡∏ï‡πâ‡∏≠‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡πÜ‡∏ñ‡∏∑‡∏≠‡∏ú‡∏•‡πÑ‡∏°‡πâ (‡∏™‡πà‡∏á‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏ï‡∏∞)", function()
    DiscordLib:Notification("DupeDurian", "‡πÄ‡∏£‡∏¥‡πà‡∏° Dupe Durian...", "‡∏ï‡∏Å‡∏•‡∏á")
    DupeDurian()
end)

----------------------------------------------------------
-- ü•ö ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î + Kick
----------------------------------------------------------
local function GiftAllEggsKick()
    if not selectedTarget then
        DiscordLib:Notification("‚ùå Error", "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô", "‡∏ï‡∏Å‡∏•‡∏á")
        return
    end

    local NANMEPLR = selectedTarget
    local P = Players
    local LP = LocalPlayer

    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ QuickSell
    RS.Remote.FishingRE:FireServer("SetEggQuickSell", {
        ["1"] = "\255",
        Diamond = false,
        ["3"] = true,
        ["2"] = false,
        ["5"] = false,
        ["4"] = false,
        ["6"] = false,
        Golden = false,
        Electirc = false,
        Fire = false,
        Dino = false,
        Snow = false
    })

    task.wait(3)

    -- Teleport ‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    local targetPlayer = P:FindFirstChild(NANMEPLR)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LP.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
        task.wait(1)
    end

    -- ‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    local AE = LP.PlayerGui.Data.Egg:GetChildren()
    for i, v in pairs(AE) do
        RS.Remote.CharacterRE:FireServer("Focus", v.Name)
        task.wait(0.1)
        RS.Remote.GiftRE:FireServer(targetPlayer)
        task.wait(0.3)
        print(i.."/"..#AE.." ‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà: "..v.Name)
    end

    DiscordLib:Notification("ü•ö ‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà", "‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß! ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å...", "‡∏ï‡∏Å‡∏•‡∏á")

    task.wait(3)
    -- ‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å
    TPService:TeleportToPlaceInstance(game.PlaceId, game.JobId)
end

main:Button("ü•ö ‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î (Kick)", function()
    DiscordLib:Notification("‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏™‡πà‡∏á‡πÑ‡∏Ç‡πà‡πÑ‡∏õ‡∏¢‡∏±‡∏á "..(selectedTarget or "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢").."...", "‡∏ï‡∏Å‡∏•‡∏á")
    GiftAllEggsKick()
end)

----------------------------------------------------------
-- üê∂ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á Idle ‚â• 300000 + Kick
----------------------------------------------------------
local function GiftPetsIdleKick()
    if not selectedTarget then
        DiscordLib:Notification("‚ùå Error", "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô", "‡∏ï‡∏Å‡∏•‡∏á")
        return
    end

    local targetPlayer = Players:FindFirstChild(selectedTarget)
    if not targetPlayer then
        DiscordLib:Notification("‚ùå Error", "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÑ‡∏°‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå", "‡∏ï‡∏Å‡∏•‡∏á")
        return
    end

    -- ‡∏£‡∏≠ Attribute DinoEventOnlineTime ‡πÇ‡∏´‡∏•‡∏î
    repeat task.wait() until LocalPlayer:GetAttribute("DinoEventOnlineTime") ~= nil

    -- ‡∏Ç‡∏≤‡∏¢‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏Å‡πà‡∏≠‡∏ô
    RS:WaitForChild("Remote"):WaitForChild("PetRE"):FireServer("SellAll", "All", "Pet")
    task.wait(1)

    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ QuickSell ‡πÑ‡∏Ç‡πà
    RS:WaitForChild("Remote"):WaitForChild("FishingRE"):FireServer("SetEggQuickSell", {
        ["1"] = "\255",
        Diamond = false,
        ["3"] = true,
        ["2"] = false,
        ["5"] = false,
        ["4"] = false,
        ["6"] = false,
        Golden = false,
        Electirc = false,
        Fire = false,
        Dino = false,
        Snow = false
    })
    task.wait(3)

    -- ‡∏Å‡∏î ProximityPrompt ‡∏Ç‡∏≠‡∏á PlayerBuiltBlocks
    for _,v in pairs(workspace.PlayerBuiltBlocks:GetChildren()) do
        if v:FindFirstChild("RootPart") and v.RootPart:FindFirstChild("ProximityPrompt") then
            fireproximityprompt(v.RootPart.ProximityPrompt)
            task.wait(0.1)
        end
    end
    task.wait(7)

    -- ‡πÄ‡∏Å‡πá‡∏ö‡∏ä‡∏∑‡πà‡∏≠‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏•‡∏ö
    local deletePets = {}
    for _, v in pairs(workspace.Pets:GetChildren()) do
        pcall(function()
            if v:IsA("Model") and v:GetAttribute("UserId") == LocalPlayer.UserId then
                if v.RootPart:FindFirstChild("GUI") and v.RootPart.GUI:FindFirstChild("IdleGUI") then
                    local petspeed = v.RootPart.GUI.IdleGUI.Speed.Text
                    local number = tonumber((petspeed:gsub("%D", "")))
                    if number and number >= 300000 then
                        table.insert(deletePets, v.Name)
                    end
                end
            end
        end)
    end

    -- ‡∏•‡∏ö‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á
    for _, petName in ipairs(deletePets) do
        RS.Remote.CharacterRE:FireServer("Del", petName)
    end

    -- Blacklist ‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏™‡πà‡∏á
    local Blacklist = {"Wolf_E1", "Kangroo_E1", "Rhino_E1", "Lion_E1", "Gorilla_E1", "Seaturtle", "Okapi", "Needlefish", "Panther", "Butterflyfish"}

    -- Teleport ‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
        task.wait(1)
    end

    -- ‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô Blacklist
    for _, v in pairs(LocalPlayer.PlayerGui.Data.Pets:GetChildren()) do
        if not table.find(Blacklist, v:GetAttribute("T")) then
            RS.Remote.CharacterRE:FireServer("Focus", v.Name)
            task.wait(0.1)
            RS.Remote.GiftRE:FireServer(targetPlayer)
            task.wait(0.3)
        end
    end

    -- Kick ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÄ‡∏Å‡∏°
    TPService:TeleportToPlaceInstance(game.PlaceId, game.JobId)
end

main:Button("üê∂ ‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á Idle ‚â•300000 (‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏™‡∏£‡πá‡∏à)", function()
    DiscordLib:Notification("GiftPets", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á‡πÑ‡∏õ‡∏¢‡∏±‡∏á "..(selectedTarget or "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢").."...", "‡∏ï‡∏Å‡∏•‡∏á")
    GiftPetsIdleKick()
end)
